<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ziggurat.inject.binding">
         <binding
            impl="org.polarsys.reqcycle.sesame.graph.RDFGraphStorageProvider"
            interface="org.polarsys.reqcycle.traceability.storage.IStorageProvider"
            name="RDF"
            priority="0">
      </binding>
      <binding
            impl="org.polarsys.reqcycle.sesame.graph.SailGraphProvider"
            interface="org.polarsys.reqcycle.traceability.storage.blueprints.graph.IGraphProvider"
            name="RDF"
            priority="0">
      </binding>
      <binding
            impl="org.polarsys.reqcycle.sesame.graph.SesameBinaryGraphProvider"
            interface="org.polarsys.reqcycle.traceability.storage.blueprints.graph.IGraphProvider"
            priority="100">
      </binding>
      <binding
            impl="org.polarsys.reqcycle.sesame.graph.RDFGraphStorageProvider"
            interface="org.polarsys.reqcycle.traceability.storage.IOneFileStorageProvider"
            name="RDF">
      </binding>
   </extension>
   <extension
         point="org.polarsys.reqcycle.uri.reachableHandler">
      <reachableHandler
            handler="org.polarsys.reqcycle.sesame.handlers.SesameHandler">
      </reachableHandler>
      <objectHandler
            handler="org.polarsys.reqcycle.sesame.handlers.SesameHandler">
      </objectHandler>
   </extension>
   <extension
         point="org.polarsys.reqcycle.traceability.builder.traceabilityAnalyser">
      <Analyser
            description="analyse the transverse traceability links created by the user"
            label="Transverse Traceability Analyser (ReqCycle)"
            visitor="org.polarsys.reqcycle.sesame.traceability.TraceabilityVisitor">
      </Analyser>
   </extension>
 	
</plugin>
